<script>
var day1Temp = 0;
var day2Temp = 0;
var day3Temp = 0;
var day4Temp = 0;
var day5Temp = 0;

$( document ).ready(function() {
  //setting today's date and time
  var d = new Date();
  var month = d.getMonth();  
  var monthNames = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
  var day = d.getDate();  
  $(".wow_weather_date").html(monthNames[month] + " " + day + " " + d.getFullYear());
  
  //Get geolocation
  if ("geolocation" in navigator) {
    
    /* geolocation is available */
    var geo = navigator.geolocation;
    geo.getCurrentPosition(function(position) {
      setLocation(position.coords.latitude, position.coords.longitude);
    });
    
  } else {
    
    /* geolocation IS NOT available */
    setLocation(0, 0);
    console.log("geo is NOT available");
  }  
   navigator.permissions && navigator.permissions.query({name: 'geolocation'}).then(function(PermissionStatus) {
        if(PermissionStatus.state == 'granted'){
              //allowed
        }else{
             $('.wow_weather_icon').html('Please enable location on your browser to view current weather.')
        }
    })
  
});

function setLocation(pos1, pos2) {   
  $.ajax( {
    url: 'https://api.darksky.net/forecast//' + pos1 + ',' + pos2,
    jsonp: "callback",
    dataType: "jsonp",
    success: function(data) {
      console.log(data);
      $(".wow_weather_timezone").html(data.timezone);
	  
	  		$(".wow_weather_day1").html("Current Temp: " + Math.floor(data.currently.apparentTemperature) + "\xB0");  
		day1Temp = Math.floor(data.currently.apparentTemperature);
		$(".wow_weather_day2").html(getWeekdayForecast(1) + "<br>" + Math.floor(data.daily.data[1].apparentTemperatureMax) + "\xB0");
		day2Temp = Math.floor(data.daily.data[1].apparentTemperatureMax);
		$(".wow_weather_day3").html(getWeekdayForecast(2) + "<br>" + Math.floor(data.daily.data[2].apparentTemperatureMax) + "\xB0");
		day3Temp = Math.floor(data.daily.data[2].apparentTemperatureMax);
		$(".wow_weather_day4").html(getWeekdayForecast(3) + "<br>" + Math.floor(data.daily.data[3].apparentTemperatureMax) + "\xB0");
		day4Temp = Math.floor(data.daily.data[3].apparentTemperatureMax);
		$(".wow_weather_day5").html(getWeekdayForecast(4) + "<br>" + Math.floor(data.daily.data[4].apparentTemperatureMax) + "\xB0");
		day5Temp = Math.floor(data.daily.data[4].apparentTemperatureMax);
	        
      switch(data.currently.icon) {         
